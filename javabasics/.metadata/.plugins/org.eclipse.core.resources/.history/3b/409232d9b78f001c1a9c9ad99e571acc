package com.te.javabasics;

public class ConstructorDefault {
    int i;
    boolean b;
    char c;
	public static void main(String[] args) {
		ConstructorDefault constructorDefault = new ConstructorDefault();
				System.out.println(constructorDefault.i);
				System.out.println(constructorDefault.b);
				System.out.println(constructorDefault.c);
         
	}

}
